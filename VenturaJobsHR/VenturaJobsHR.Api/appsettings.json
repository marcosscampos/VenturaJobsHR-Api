{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "MongoDb": {
      "Uri": "mongodb://localhost:27017?retryWrites=false",
      "DatabaseName": "VenturaJobs"
    }
  },
  "Keycloak": {
    "Url": "http://localhost:8080/auth",
    "Realm": "ventura-jobs",
    "ClientId": "ventura-jobs-auth",
    "RequireHttps": false
  },
  "AllowedHosts": "*"
}
